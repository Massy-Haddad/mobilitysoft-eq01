# Configuration de l'application MobilitySoft
APP_NAME=MobilitySoft — Serveur de prédiction de trafic (FR)
API_V1_STR=/api/v1
LOG_LEVEL=INFO

# Configuration de la base de données PostgreSQL
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/mobilitysoft_db

# Configuration du rate limiting
RATE_LIMIT_PER_MIN=60

# Configuration du modèle de prédiction
MODEL_SEED=42
